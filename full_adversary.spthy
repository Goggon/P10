restriction Only_once:
"
  All type #i.
    Reach(type) @ i ==> not(Ex #j. Reach(type) @j & not(#j = #i) )
"*/
lemma Reach_1:
exists-trace
"
  Ex #i. Reach('HRR') @ i
"

lemma Reach_2:
exists-trace
"
  Ex #i. Reach('CHC') @ i
"
lemma Reach_3:
exists-trace
"
  Ex #i. Reach('SH') @ i
"
lemma Reach_4:
exists-trace
"
  Ex #i. Reach('EE') @ i
